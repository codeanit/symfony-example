<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>        
        <service id="queue" class="ApiBundle\Model\Queue" lazy="true">            
            <argument type="service" id="service_container" />         
        </service>
        <service id="log" class="ApiBundle\Model\Log" lazy="true">            
            <argument type="service" id="service_container" />         
        </service>
         <service id="connection" class="ApiBundle\Model\DB" lazy="true">            
            <argument type="service" id="service_container" />         
        </service>
         <service id="transnetwork" class="Api\PayoutBundle\Library\TransNetwork" lazy="true">
            <argument type="service" id="service_container" />         
        </service>
        <service id="tb" class="Api\PayoutBundle\Library\TBConnectionModel" lazy="true">
            <argument type="service" id="service_container" />
        </service>
    </services>
    
</container>
